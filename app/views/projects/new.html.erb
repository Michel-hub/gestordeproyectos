<h1>Projects#new</h1>


<div>

<%= link_to "Volver", root_path %>

</div>

<%= form_with(model: @project, local: true) do |form| %>

  <div class="form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :descripcion %>
    <%= form.text_field :description, class: "form-control-file" %>
  </div>
  
  <div class="form-group">
    <%= form.label :begin %>
    <%= form.date_field :begin %>
  </div>

  <div class="form-group">
    <%= form.label :end %>
    <%= form.date_field :end %>
  </div>

  <div class="form-group">
    <%= form.label :state %>
    <%= form.select :state, Project.states.keys %>
  </div>

  <div class="actions">
    <%= form.submit class: "btn btn-primary float-right" %>
  </div>
 

<% end %>
